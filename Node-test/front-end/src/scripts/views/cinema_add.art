<div class="col-lg-6">
    <section class="panel">
        <header class="panel-heading">
            <trans oldtip=" Basic Forms " newtip="添加影院信息">添加影院信息</trans>
        </header>
        <div class="panel-body">
            <form role="form" id="cinema-form">
                <div class="form-group">
                    <label for="exampleInputEmail1">
                        <trans oldtip="CinemaName" newtip="影院名称">影院名称</trans>
                    </label>
                    <input type="text" class="form-control" id="CinemaName" name="cinemaName" placeholder="请输入影院名称">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">
                        <trans oldtip="CinemaLocation" newtip="影院位置">影院位置</trans>
                    </label>
                    <input type="text" class="form-control" id="CinemaLocation" name="cinemaLocation" placeholder="请输入影院位置">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">
                        <trans oldtip="CinemaPhone" newtip="影院电话">影院电话</trans>
                    </label>
                    <input type="text" class="form-control" id="CinemaPhone" name="cinemaPhone" placeholder="请输入影院电话">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">
                        <trans oldtip="CinemaServe" newtip="影院服务">影院服务（三个）</trans>
                    </label>
                    <input type="text" class="form-control" id="CinemaServe" name="cinemaServe1" placeholder="请输入影院服务">
                    <input type="text" class="form-control" id="CinemaServe" name="cinemaServe2" placeholder="请输入影院服务">
                    <input type="text" class="form-control" id="CinemaServe" name="cinemaServe3" placeholder="请输入影院服务">
                </div>
                <!-- <div class="form-group">
                    <label for="exampleInputFile">
                        <trans oldtip="File input" newtip="文件输入">图片</trans>
                    </label>
                    <input type="file" id="exampleInputFile">
                  
                </div> -->
             
                <button class="btn btn-default"  id="posadd-back" type="button">
                    <trans oldtip="Cancel" newtip="取消" style="">返回</trans>
                </button>
                <button type="button" id="posadd-submit" class="btn btn-primary pull-right">
                    <trans oldtip="Submit" newtip="提交">提交</trans>
                </button>
            </form>

        </div>
    </section>
</div>