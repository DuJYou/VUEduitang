<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                影院列表

                <button id="btn-add" class="btn btn-info" title="Bootstrap 3 themes generator "
                    style="padding: 4px 12px;float:right">
                    <span class="icon_plus_alt2"></span>
                    <trans oldtip=" Info" newtip="添加">添加</trans>
                </button>
                <ul class="nav top-menu" style="padding:7px 24px;float:right">
                    <li style="border: none;">
                        <form class="navbar-form">
                            <input id="search" class="form-control" placeholder="搜索" type="text">
                        </form>
                    </li>
                </ul>
            </header>

            <table class="table table-striped table-advance table-hover">
                <tbody>
                    <tr>
                        <th><i class="icon_document_alt"></i>编号</th>
                        <th><i class="fa fa-home"></i> 影院名称</th>
                        <th><i class="icon_pin_alt"></i> 影院地址</th>
                        <th><i class="icon_mobile"></i> 影院电话</th>
                        <th><i class="icon_profile"></i>影院服务</th>
                        <th><i class="icon_cogs"></i> 操作</th>
                    </tr>
                    {{each list}}
                    <tr>
                        <td>{{$index + 1}}</td>
                        <td>{{$value.cinemaName}}</td>
                        <td>{{$value.cinemaLocation}}</td>
                        <td>{{$value.cinemaPhone}}</td>
                        <td>
                            <span class="label label-warning">{{$value.cinemaServe1}}</span>
                            <span class="label label-warning">{{$value.cinemaServe2}}</span>
                            <span class="label label-warning">{{$value.cinemaServe3}}</span>
                        </td>

                        <td>
                            <div class="btn-group">
                                <!-- <i class="btn btn-primary" href="#"><i class="icon_plus_alt2"></i></a> -->
                                <button data-id="{{$value._id}}" type="button" class="btn btn-success btn-updata"><span
                                        class="icon_check_alt2"></span>
                                    <trans oldtip="Submit" newtip="修改">修改</trans>
                                </button>
                                <button data-id="{{$value._id}}" type="button" class="btn btn-danger btn-remove"><span
                                        class="icon_close_alt2"></span>删除</button>
                            </div>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>

            </table>
            {{if list.length > 0 && from === 'list' }}
            <div id="box-footer" class="box-footer clearfix text-center">
                <ul class="pagination pagination-sm no-margin pull-right">
                    <li><a href="javascript:void(0)" class="page-prev">&laquo;</a></li>
                    {{each pageCount}}
                    {{if currentPage === $value}}
                    <li class="active">
                        {{else}}
                    <li>
                        {{/if}}
                        <a href="javascript:void(0)" class="page-number">{{$value}}</a></li>
                    {{/each}}
                    <li><a href="javascript:void(0)" class="page-next">&raquo;</a></li>
                </ul>
            </div>
            {{/if}}
        </section>
    </div>
</div>